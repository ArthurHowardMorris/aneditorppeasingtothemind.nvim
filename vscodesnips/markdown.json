{
	"inline_equation": {
		"prefix": "$",
		"body": [
			"\\$$1\\$$0"
		],
		"description": "inline equation"
	},
	"equation": {
		"prefix": "$$",
		"body": [
			"\\$\\$",
			"$1",
			"\\$\\$",
			"$0"
		],
		"description": "equation"
	},
	"snip2": {
		"prefix": "mark2",
		"body": [
			"multi $1",
			"line $2",
			"snippet$0"
		]
	},
	"md_blog_header": {
		"prefix": "blog",
		"body": [
			"---",
			"published: ${1:false}",
			"title: \"$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE\"",
			"category: \"${2:writing coding talking}\"",
			"tags:",
			"\t- ${3:tag}",
			"---"
		],
		"description": "a yaml header for a blog post"
	},
	"q1toqn": {
		"prefix": "qn",
		"body": [
			"q_1,q_2,\\dots,q_n"
		],
		"description": "values of q_1 to q_n"
	}
}
